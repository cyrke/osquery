table_name("ipmi_threshold_sensors")
description("IPMI readings for threshold sensors.")
schema([
    Column("name", TEXT, "Name of threshold sensor"),
    Column("value", TEXT, "Value read from sensor"),
    Column("type", TEXT, "Name of sensor type"),
    Column("threshold_out_of_range", INTEGER,
      "Boolean value indicating whether reading is out-of-range of threshold"),
])
implementation("linux/ipmi@genIPMIThresholdSensors")
